{
  "name": "clean-node-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "private": false,
  "scripts": {
    "test": "jest --watchAll --silent --noStackTrace",
    "test:staged": "jest --passWithNoTests --silent --noStackTrace",
    "test:ci": "jest --noStackTrace"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run test:ci"
    }
  },
  "lint-staged": {
    "*.js": [
      "standard --fix",
      "npm run test:staged",
      "git add"
    ]
  },
  "standard": {
    "env": [
      "jest"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lukyo17/clean-node-api.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/lukyo17/clean-node-api/issues"
  },
  "homepage": "https://github.com/lukyo17/clean-node-api#readme",
  "devDependencies": {
    "husky": "^4.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.1",
    "standard": "^16.0.2"
  },
  "dependencies": {}
}
